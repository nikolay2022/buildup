<idea-plugin>
  <id>nikolay2022.idea.plugin.notification</id>
  <name>BuildUp</name>
  <version>1.2.4</version>
  <vendor email="nik.yakushov@gmail.com" url="https://github.com/nikolay2022/myPlugin">Nikolay Yakushov</vendor>
  <description>Receive notifications about the completion of the assembly in telegrams. Build finish telegram notification
        Bot name @irlix_buildUp_bot.
        Link bot: https://t.me/irlix_buildUp_bot
    </description>
  <idea-version since-build="193"/>
  <depends>com.intellij.modules.java</depends>
  <depends>com.intellij.modules.lang</depends>
  <depends optional="true" config-file="android_config.xml">org.jetbrains.android</depends>
  <depends>com.intellij.modules.platform</depends>
  <extensions defaultExtensionNs="com.intellij">
    <compiler.task execute="BEFORE" implementation="project.BeforeCompile"/>
  </extensions>
  <actions>
    <action id="your.action.id" class="project.Action" text="Change Id" description="Action description" icon="AllIcons.Providers.Teradata">
      <add-to-group group-id="ToolbarRunGroup" anchor="first"/>
    </action>
  </actions>
  <applicationListeners>
    <listener class="project.MyProjectManagerListener" topic="com.intellij.openapi.project.ProjectManagerListener"/>
  </applicationListeners>
  <project-components>
    <component>
      <implementation-class>project.Drumroll</implementation-class>
    </component>
  </project-components>
</idea-plugin>
